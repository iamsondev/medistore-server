model OrderItem {
  id         String   @id @default(uuid())
  orderId    String
  medicineId String
  quantity   Int
  price      Float
  order      Order    @relation(fields: [orderId], references: [id])
  medicine   Medicine @relation(fields: [medicineId], references: [id])

  @@index([orderId])
  @@index([medicineId])
}
